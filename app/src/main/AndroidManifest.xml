<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.crmempty">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.CRMEmpty">
        <activity android:name=".SalesOrderAdd">
        </activity>
        <activity android:name=".ItemAdd">
        </activity>
        <activity android:name=".CustomerAdd">
        </activity>
        <activity android:name=".ItemCatAdd">
        </activity>
        <activity android:name=".WarehouseAdd">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>